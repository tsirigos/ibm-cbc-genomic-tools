#!/bin/tcsh

##
## USAGE: copy_source
##

if ($#argv < 0) then
  usage $0
  exit
endif

ua cp

cp ~/Code/basic/core.cpp src
cp ~/Code/basic/core.h src
cp ~/Code/basic/gzstream.c src
cp ~/Code/basic/gzstream.h src

cp ~/Code/genomic_tools/genomic_intervals.cpp src
cp ~/Code/genomic_tools/genomic_intervals.h src
cp ~/Code/genomic_tools/genomic_overlaps.cpp src
cp ~/Code/genomic_tools/genomic_regions.cpp src
cp ~/Code/genomic_tools/genomic_scans.cpp src
cp ~/Code/genomic_tools/genomic_apps.cpp src
cp ~/Code/genomic_tools/genomic_apps.rscripts.h src

cp ~/Code/tools/matrix.cpp src
cp ~/Code/tools/permutation_test.cpp src
cp ~/Code/tools/vectors.cpp src

cp ~/Code/scripts/sortreg bin
cp ~/Code/scripts/sortreg_check bin
cp ~/Code/scripts/sortbed bin
cp ~/Code/scripts/sortbed_check bin

